{
    "name": "imdad/cakephp3.x-mongodb",
    "description": "NoSql plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.0",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "NoSql\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NoSql\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
